{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*":[
        "./src/assets/*"
      ],
      "@components/*":[
        "./src/components/*"
      ],
      "@screens/*":[
        "./src/screens/*"
      ],
      "@theme/*":[
        "./src/theme/*"
      ],
      "@utils/*":[
        "./src/utils/*"
      ],        
    }
  }
}
